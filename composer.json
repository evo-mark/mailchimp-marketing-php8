{
    "name": "evo-mark/mailchimp-marketing-php8",
    "version": "4.0.0",
    "description": "PHP 8 compatible fork of the official PHP client for Mailchimp Marketing",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://evomark.co.uk",
    "license": "proprietary",
    "authors": [
        {
            "name": "Mailchimp",
            "homepage": "https://github.com/evo-mark/mailchimp-marketing-php8"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "guzzlehttp/guzzle": "^6.4 || ^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": {
            "MailchimpMarketing\\" : "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": { "MailchimpMarketing\\" : "test/" }
    }
}
